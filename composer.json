{
  "name": "tourze/workerman-relay-worker",
  "description": "Relay Worker",
  "license": "MIT",
  "require": {
    "tourze/workerman-connection-pipe": "0.0.*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "tourze/phpunit-base": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\RelayWorker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\RelayWorker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}